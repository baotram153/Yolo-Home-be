{
  "name": "backend",
  "version": "1.0.0",
  "proxy": "http://localhost:3001",
  "description": "Backend for SmartHome app",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "predev": "npm run build",
    "dev": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon dist/index.js\"",
    "deploy": "npx prisma generate && npx prisma migrate deploy && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "npx prisma migrate dev --name init && npx prisma generate",
    "seed": "npx prisma db seed"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "axios": "^1.9.0",
    "backend": "file:",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mqtt": "^5.10.4",
    "multer": "^1.4.5-lts.2",
    "prisma": "^6.5.0",
    "rimraf": "^6.0.1",
    "socket.io": "^4.8.1",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9"
  }
}
